<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leafpicrevivedk">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:configChanges="locale"
        android:theme="@style/AppTheme">

        <activity android:name=".Ui.Settinform.inpublic.Insecurity"
            android:parentActivityName=".Ui.NavigationView.Setting"
            />
        <activity android:name=".Ui.NavigationView.Setting"
            android:parentActivityName=".Ui.Forthfristscreen.MainActivity"
            />
        <!--            android:configChanges="orientation|screenSize"-->

        <activity
            android:name=".Ui.Forthfristscreen.MainActivity"
          >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Ui.wheniopenfolder.PhotosActivity"
            android:parentActivityName=".Ui.Forthfristscreen.MainActivity"
            >
            <intent-filter>
                <action android:name="luminous.ACTION_PICK" />
                <action android:name="luminous.ACTION_MULTIPLE_PICK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>